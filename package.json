  "author": "Justin Bay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jwbay/tslint-misc-rules/issues"
  },
  "homepage": "https://github.com/jwbay/tslint-misc-rules#readme",
  "dependencies": {
    "tslint": "^4.1.1",
    "typescript": ">=1.8 <2.2"
  },
  "devDependencies": {
    "tslint-eslint-rules": "^3.2.0",
    "tslint-microsoft-contrib": "^4.0.0"
  }
